@{
}

@model IEnumerable<LaurenDaines_FilmCollection.Models.ApplicationResponse> 

<div class="text-center">
    <h1 class="display-4">@ViewBag.Title</h1>
</div>

<br />

<table id="movies">

    <tr>
        <th>Category</th>
        <th>Title</th>
        <th>Year</th>
        <th>Director</th>
        <th>Rating</th>
        <th>Edited</th>
        <th>Lent To</th>
        <th>Notes</th>
    </tr>

    <tbody>
        @foreach (LaurenDaines_FilmCollection.Models.ApplicationResponse x in Model)
        {
            <tr>
                <td>@x.Category</td>
                <td>@x.Title</td>
                <td>@x.Year</td>
                <td>@x.Director</td>
                <td>@x.Rating</td>
                <td>@x.Edited</td>
                <td>@x.LentTo</td>
                <td>@x.Notes</td>
            </tr>
        }

    </tbody>

</table>